<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rectangle"
    tools:context="com.example.motelcontrol.motelcontrol.lock_door"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="25dp">

    <com.example.motelcontrol.motelcontrol.viewclasess.Circle
        android:id="@+id/loading_circle1"
        android:layout_width="60dp"
        android:layout_marginTop="510dp"
        android:layout_marginStart="120dp"
        android:layout_height="60dp" />

    <com.example.motelcontrol.motelcontrol.viewclasess.Circle
        android:id="@+id/loading_circle2"
        android:layout_width="60dp"
        android:layout_marginTop="510dp"
        android:layout_marginStart="150dp"
        android:layout_height="60dp" />

    <com.example.motelcontrol.motelcontrol.viewclasess.Circle
        android:id="@+id/loading_circle3"
        android:layout_width="60dp"
        android:layout_marginTop="510dp"
        android:layout_marginStart="180dp"
        android:layout_height="60dp" />

    <com.example.motelcontrol.motelcontrol.viewclasess.Circle
        android:id="@+id/loading_circle4"
        android:layout_width="60dp"
        android:layout_marginTop="510dp"
        android:layout_marginStart="210dp"
        android:layout_height="60dp" />

    <Button
        android:id="@+id/button_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="205dp"
        android:layout_marginLeft="147dp"
        android:layout_marginStart="147dp"
        android:text="Button" />

</RelativeLayout>
